{
    "name": "jelix/news-module",
    "type": "jelix-module",
    "description": "A simple module to display some news",
    "keywords": ["module", "jelix", "news"],
    "homepage": "https://jelix.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Laurent Jouanneau",
            "email": "laurent@jelix.org"
        }
    ],
    "require": {
        "jelix/feeds-module": "~1.7.6",
        "jelix/wikirenderer-plugin": "~1.7.3"
    },
    "conflict": {
       "jelix/jelix": "<1.8.0"
    },
    "minimum-stability": "stable",
    "extra" : {
        "branch-alias": {
            "dev-master": "2.0-dev",
            "dev-1.0.x": "1.0-dev"
        },
        "jelix" : {
            "modules": [
                "news", "news_admin"
            ]
        }
    }
}
